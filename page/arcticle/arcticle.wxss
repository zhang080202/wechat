.container {
  height: 100%;
}
.page-head {
  height: 30%;
}
.page-head-image {
  height: 100%;
  width: 100%;
}
.page-body {
  height: 70%;
}
.page-body-item {
  height: 150rpx;
  width: 100%;
  /* display: flex; */
  background-color: white;
  margin-top: 5rpx;
}
.page-body-item-cnetent {
  height: 100%;
  width: 70%;
  display: table;
  overflow: hidden; 
}
.page-body-item-image {
  height: 100%;
  width: 30%;
}
.page-body-image {
  height: 90%;
  width: 90%;
  margin-top: 4%;
  margin-left: 4%;
}
.page-body-item-cnetent-label {
  display: table-cell;
  vertical-align: middle;
  font-family: 'Times New Roman', Times, serif;
  font-weight: 700;
  font-size: small;
  word-break: break-all;
  word-wrap:break-word;
  margin-left: 15rpx;
}
.page-body-item-navigator {
  height: 100%;
  width: 100%;
}
.page-body-item-navigator-view {
  display: flex;
  height: 100%;
  width: 100%;
}